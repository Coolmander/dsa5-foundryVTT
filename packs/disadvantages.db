{"_id":"KoWqLDh2ae5EaQqW","name":"Fettleibig","permission":{"default":0,"YrSB7QTCbkzI1YeL":3},"type":"disadvantage","data":{"description":{"type":"String","label":"Description","value":"Der Nachteil hat zur Folge, dass Proben auf Klettern, Körperbeherrschung, Tanzen und Verbergen um 1 erschwert sind.<br/><br/>\nPublikation: Regelwerk Seite 171"},"gmdescription":{"type":"String","label":"Description","value":""},"APValue":{"type":"Number","label":"APValue","value":null},"max":{"type":"Number","label":"Max","value":0},"requirements":{"type":"String","label":"Requirements","value":"Nachteil Behäbig"},"step":{"value":1,"type":"Number","label":"step"},"effect":{"type":"String","label":"effect","value":""}},"flags":{},"img":"icons/commodities/bones/skull-hollow-white.webp","effects":[]}
